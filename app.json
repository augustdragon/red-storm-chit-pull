{
  "expo": {
    "name": "Red Storm Chit Pull",
    "slug": "red-storm-chit-pull",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2c3e36"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.augustdragon.redstormchitpull",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2c3e36"
      },
      "package": "com.augustdragon.redstormchitpull"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "eas": {
        "projectId": "a291a35f-581e-4a60-83cb-8863e1ff0bd6"
      }
    }
  }
}
